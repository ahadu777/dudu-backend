{
  "name": "express-typescript-api",
  "version": "1.0.0",
  "description": "Modern Express API with TypeScript, MySQL and Swagger",
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "test:e2e": "newman run docs/postman_e2e.json -e postman_env.local.json --reporters cli,junit --reporter-junit-export reports/newman/e2e.xml",
    "test:e2e:ci": "mkdir -p reports/newman && npm run test:e2e",
    "example:us001": "ts-node examples/us001.ts",
    "example:us002": "ts-node examples/us002.ts",
    "example:us003": "ts-node examples/us003.ts",
    "example:all": "ts-node examples/all-stories.ts",
    "sdk:generate": "npx openapi-typescript-codegen --input openapi/openapi.json --output sdk --client fetch",
    "validate:integration": "node scripts/integration-proof-validator.js",
    "validate:autonomy": "node scripts/fresh-ai-readiness.js"
  },
  "keywords": [
    "express",
    "typescript",
    "mysql",
    "swagger",
    "api"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.10.5",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "bcrypt": "^6.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "envalid": "^8.1.0",
    "express": "^5.1.0",
    "helmet": "^8.0.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "mysql2": "^3.11.5",
    "reflect-metadata": "^0.2.2",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "typeorm": "^0.3.20",
    "typescript": "^5.7.2",
    "winston": "^3.18.3"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.19.1",
    "@typescript-eslint/parser": "^8.19.1",
    "eslint": "^9.18.0",
    "newman": "^6.2.1",
    "openapi-typescript-codegen": "^0.29.0",
    "prettier": "^3.4.2",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0"
  }
}
